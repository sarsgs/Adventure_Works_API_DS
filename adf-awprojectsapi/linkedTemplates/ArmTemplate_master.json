{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-awprojectsapi"
		},
		"AwDatalake_conn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AwDatalake_conn'"
		},
		"AwDatalake_conn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://awprojectsapi.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://gitconnection.vault.azure.net/"
		},
		"GitServer_Data_Source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"GitServer_Data_Source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sarsgs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-awprojectsapi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AwDatalake_conn_accountKey": {
						"value": "[parameters('AwDatalake_conn_accountKey')]"
					},
					"AwDatalake_conn_properties_typeProperties_url": {
						"value": "[parameters('AwDatalake_conn_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"GitServer_Data_Source_properties_typeProperties_url": {
						"value": "[parameters('GitServer_Data_Source_properties_typeProperties_url')]"
					},
					"GitServer_Data_Source_properties_typeProperties_userName": {
						"value": "[parameters('GitServer_Data_Source_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}